{
  "name": "leepe/api-plex",
  "keywords": ["plex", "api"],
  "homepage": "https://github.com/leepeuker/plex-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Lee Peuker",
      "email": "lee.peuker@protonmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.4",
    "guzzlehttp/psr7": "^1.6",
    "php-http/guzzle6-adapter": "^2.0",
    "php-http/message": "^1.8"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.19",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "phpunit/phpunit": "^8",
    "psalm/plugin-phpunit": "^0.7.0",
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "PlexApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PlexApi\\Tests\\Unit\\": "tests/unit"
    }
  },
  "config": {
    "sort-packages": true
  }
}
